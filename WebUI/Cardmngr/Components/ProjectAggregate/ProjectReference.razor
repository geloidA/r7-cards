@inject NavigationManager NavigationManager

<a class="fw-light small py-1" title="@Project.Title" href=@($"/project/{Project.Id}")>
    <div class="ref-container">
        @Project.Title
        <FluentIcon Value="@(new Icons.Regular.Size12.ArrowRight())" />
    </div>
</a>

@code {
    [Parameter] public Project Project { get; set; } = null!;
}
